{
  "name": "@sudoplatform/sudo-api-client",
  "version": "12.2.0",
  "author": "Anonyome Labs, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudoplatform/sudo-api-client-js.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf ./lib ./docs ./cjs ./types",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"{bin,integration-tests,src}/**/*.{ts,tsx}\"",
    "lint:prettier": "prettier -l \"**/*.{js,json,ts,yml}\"",
    "test": "jest",
    "build": "yarn clean && yarn build:transpile && yarn build:verify && yarn build:docs",
    "build:verify": "yarn audit-with-suppressions && yarn lint && yarn test",
    "build:transpile": "concurrently \"yarn build:transpile:declarations\" \"yarn build:transpile:cjs\" \"yarn build:transpile:esm\"",
    "build:transpile:cjs": "tsc --project tsconfig.commonjs.json",
    "build:transpile:esm": "tsc",
    "build:transpile:declarations": "tsc --project tsconfig.declarations.json",
    "build:docs": "typedoc --excludePrivate --out docs src/index.ts"
  },
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "default": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "cjs",
    "lib",
    "docs",
    "types"
  ],
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14"
  },
  "devDependencies": {
    "@sudoplatform/sudo-common": "^8.5.4",
    "@sudoplatform/sudo-user": "^13.0.2",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "aws-appsync": "^4.1.9",
    "concurrently": "^7.6.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "fp-ts": "^2.16.0",
    "graphql": "^15.8.0",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "prettier": "^2.8.8",
    "redux": "^4.2.1",
    "ts-jest": "^28.0.8",
    "ts-mockito": "^2.6.1",
    "typedoc": "^0.24.8",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-cache-inmemory": "^1.6.6",
    "glob-parent": "^5.1.2",
    "node-fetch": "^2.6.9",
    "react-native/**/mem": "^4.0.0",
    "react-native/**/node-notifier": "^8.0.1",
    "react-native/**/braces": "^2.3.1",
    "react-native/**/merge": "^2.1.1",
    "react-native/**/yargs-parser": "^18.1.3",
    "hermes-engine": "^0.11.0",
    "shell-quote": "^1.8.1",
    "uuid": "^9.0.0",
    "aws-appsync": "^4.1.9",
    "json5": "^2.2.2"
  },
  "peerDependencies": {
    "@sudoplatform/sudo-common": "^8.3.1",
    "@sudoplatform/sudo-user": "^13.0.0",
    "aws-appsync": "^4.1.9",
    "io-ts": "^2.2.20"
  }
}
