    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "prettier": "^3.6.2",
